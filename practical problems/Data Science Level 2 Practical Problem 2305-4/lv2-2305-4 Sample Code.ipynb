   "source": [
    "df4 = set2.copy()\n",
    "\n",
    "skew_check = pd.DataFrame(index= df4.columns)\n",
    "\n",
    "for i in df4.columns:\n",
    "    m2 = ((df4[i]- df4[i].mean())**2).mean()\n",
    "    m3 = ((df4[i]- df4[i].mean())**3).mean()\n",
    "    skew_check.loc[i,'skew'] = m3/m2**(3/2)"
   ]
